<Window x:Class="ScrabbleSolver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="850" Width="1000" WindowStartupLocation="CenterScreen">
    <Grid>
        <!-- Menu with actions -->
        <Menu Height="22" HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <MenuItem Header="File">
                <MenuItem Header="Open last game"/>
                <MenuItem Header="Open game"/>
                <MenuItem Header="Save game"/>
                <MenuItem Header="Reset game"/>
            </MenuItem>
            <MenuItem Header="Action">
                <MenuItem Header="Clear letters"/>
                <MenuItem Header="Override scoring"/>
                <MenuItem Header="Override player turn"/>
                <MenuItem Header="Override board" Click="OverrideBoardClick"/>
                <MenuItem Header="Add player" Click="AddPlayerMenuItemClick"/>
                <MenuItem x:Name="DebugModeItem" Header="Debug mode" IsCheckable="True" Click="DebugModeItem_OnClick"/>
            </MenuItem>
        </Menu>
        
        <Button x:Name="FirstDebugButton" Margin="20,100,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="100" Content="+250" Click="FirstDebugButton_OnClick" Visibility="Hidden"/>
        <Button x:Name="NextDebugButton" Margin="140,100,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="100" Content="Next result" Visibility="Hidden" Click="NextDebugButtonSelected"/>
        <TextBlock x:Name="FirstDebugLabel" Margin="260,100,0,0" Text="No results yet..." HorizontalAlignment="Left" VerticalAlignment="Top" Visibility="Hidden"/>
        
        <!-- Greeting -->
        <TextBlock HorizontalAlignment="Left" Margin="10,27,0,0" TextWrapping="Wrap" Text="ScrabbleSolver" VerticalAlignment="Top" FontSize="24"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,64,0,0" TextWrapping="Wrap" Text="From CyanCoding" VerticalAlignment="Top"/>

        <!-- Scoring menu -->
        <TextBlock HorizontalAlignment="Right" VerticalAlignment="Bottom" Text="Scoring" FontSize="22" Margin="0,0,10,320" Width="250"/>
        <Border BorderBrush="Black" BorderThickness="2" Height="300" Width="250" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,10,10">
            <Grid x:Name="ScoringGrid" Margin="5,5,5,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="180"/>
                    <ColumnDefinition Width="50"/>
                </Grid.ColumnDefinitions>
                <!-- This gets filled automatically by Players.AddPlayer() -->
            </Grid>
        </Border>
        
        <!-- Player turns -->
        <Border BorderBrush="Black" BorderThickness="2" Height="100" Width="250" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,200,10,0">
            <Grid Margin="10">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>

                <TextBlock x:Name="TurnTextBox" Text="Turn: " FontSize="22" Grid.ColumnSpan="2"/>
                <Button Grid.Column="0" x:Name="NextTurn" Height="30" VerticalAlignment="Bottom" Content="Next turn" FontSize="16" Width="100" Click="nextTurn_Click"/>
                <Button x:Name="TurnEditTable" Height="30" Grid.Column="1" VerticalAlignment="Bottom" Content="Edit board" FontSize="16" Width="100" Click="editBoard_Click"/>
            </Grid>
        </Border>

        <!-- Your letters -->
        <Border BorderBrush="Black" BorderThickness="2" Height="100" Width="250" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,50,10,0">
            <Grid Margin="10">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                
                <TextBox x:Name="YourLettersBox" HorizontalAlignment="Stretch" Grid.ColumnSpan="3" TextWrapping="Wrap" Text="LETTERS" HorizontalContentAlignment="Center" MaxLength="7" VerticalAlignment="Top" FontSize="30" IsReadOnly="True"/>
                <Button Grid.Column="0" x:Name="SetButton" Height="30" Content="Set" FontSize="16" VerticalAlignment="Bottom" ToolTip="Change the letters in your hand" Click="SetButton_Click"/>
                <Button Height="30" Content="Fill" FontSize="16" Grid.Column="1" VerticalAlignment="Bottom" ToolTip="Find the best spot for your letters" Click="FillButton_Click"/>
                <Button Height="30" Content="Shuffle" FontSize="16"  Grid.Column="2" VerticalAlignment="Bottom" ToolTip="Scramble your letters" Click="ShuffleButton_Click"/>
            </Grid>
        </Border>

        <!-- Grid board -->
        <Grid x:Name="BoardGrid" Width="675" Height="675" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,0,0,10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
                <ColumnDefinition Width="45"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
